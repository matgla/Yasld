{
  "dependencies": [
    {
      "link": "https://github.com/matgla/CMakeUtils.git",
      "type": "git",
      "version": "master",
      "target": "CMakeUtils",
      "options": {
        "is_cmake_library": true
      }
    },
    {
      "link": "https://github.com/matgla/EmbeddedUtilityLibraries.git",
      "type": "git",
      "version": "master",
      "target": "eul",
      "options": {
        "cmake_variables": {
          "DISABLE_TESTS": "ON"
        }
      }
    },
    {
      "link": "https://github.com/mpaland/printf.git",
      "type": "git",
      "version": "v4.0.0",
      "target": "printf",
      "options": {
        "create_library": {
          "type": "STATIC",
          "sources_filter": [
            "*.c"
          ],
          "include_directories": [
            "."
          ],
          "sources_directory": ".",
          "compile_definitions": ""
        }
      }
    }
  ]
}
